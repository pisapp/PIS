<h1>New material_exchange</h1>

<% form_for(@material_exchange) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :type %><br />
    <%= f.text_field :type %>
  </p>
  <p>
    <%= f.label :inventory_id %><br />
    <%= f.text_field :inventory_id %>
  </p>
  <p>
    <%= f.label :material_id %><br />
    <%= f.text_field :material_id %>
  </p>
  <p>
    <%= f.label :weight_unit_id %><br />
    <%= f.text_field :weight_unit_id %>
  </p>
  <p>
    <%= f.label :size %><br />
    <%= f.text_field :size %>
  </p>
  <p>
    <%= f.label :reason %><br />
    <%= f.text_area :reason %>
  </p>
  <p>
    <%= f.label :created_date %><br />
    <%= f.datetime_select :created_date %>
  </p>
  <p>
    <%= f.label :created_by %><br />
    <%= f.text_field :created_by %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', material_exchanges_path %>