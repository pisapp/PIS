<h1>Listing material_types</h1>

<table>
  <tr>
    <th>Code</th>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% @material_types.each do |material_type| %>
  <tr>
    <td><%=h material_type.code %></td>
    <td><%=h material_type.name %></td>
    <td><%=h material_type.description %></td>
    <td><%= link_to 'Show', material_type %></td>
    <td><%= link_to 'Edit', edit_material_type_path(material_type) %></td>
    <td><%= link_to 'Destroy', material_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New material_type', new_material_type_path %>